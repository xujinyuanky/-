#include<iostream>
using namespace std;

int shunxu_search(int a[], int n, int key) {
	for (int i = 0; i < n; i++) {
		if (a[i] == key)
			return i;
	}
	return -1;
}

int main() {
	int a[] = { 2,5,6,7,8,10,16,22,23,70 };
